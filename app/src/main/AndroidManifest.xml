<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.iessanclemente.a14felipecm.proyecto_a14felipecm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/api_key"/>

        <activity
            android:name=".Principal"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Centro"
            android:label="@string/title_activity_centro"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Oferta_Educativa"
            android:label="@string/title_activity_oferta__educativa"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Alumno"
            android:label="@string/title_activity_alumno"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Contacto"
            android:label="@string/title_activity_contacto"
            android:theme="@style/AppTheme"></activity>
    </application>


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


</manifest>
